edgeStyle transitionStyle(1){
	appearanceProvider("info.scce.cinco.product.scchart.appearance.TransitionAppearance")
	appearance transitionAppearance
	decorator {
		location (1.0) // at the end of the edge
		ARROW
		appearance transitionAppearance 
	}
	decorator {
		location (0.5)
		movable
		text {
			value "%s"
		}
	}
}

