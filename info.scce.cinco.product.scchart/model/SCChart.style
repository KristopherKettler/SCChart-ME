nodeStyle rootStateStyle (1) {
	roundedRectangle {
		appearance superStateAppearance
		size(800,1000)
		corner(15,15)
		text {
			appearance textFont
			position (CENTER,TOP 3)
			value "%1$s"
		}
	}
}

